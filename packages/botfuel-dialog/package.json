    "express": "^4.15.4",
    "express-handlebars": "^3.0.0",
    "fs": "0.0.1-security",
    "fs-extra": "^4.0.2",
    "lodash": "^4.17.4",
    "logtown": "2.1.1",
    "logtown-winston": "^1.0.0",
    "mongodb": "^2.2.31",
    "natural": "^0.5.4",
