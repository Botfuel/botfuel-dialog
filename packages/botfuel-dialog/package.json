  },
  "license": "Apache-2.0",
  "files": [
    "build"
  ],
  "bin": {
    "botfuel-run": "./build/run.js",
    "botfuel-train": "./build/train.js",
    "botfuel-clean": "./build/clean.js"
  },
  "scripts": {
    "compile": "rm -rf build; ../../node_modules/.bin/babel src --out-dir build; cp -r src/corpora/*.txt build/corpora/",
